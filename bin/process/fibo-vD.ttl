        skos:prefLabel      "Futures Initial Trading Margin"@en .

fibo-v-der:ObservableValue
        rdf:type          skos:Concept ;
        rdfs:isDefinedBy  fibo-der-drc-bsc:ObservableValue ;
        dct:isPartOf      <https://spec.edmcouncil.org/fibo/DER/AboutDER/DERSpecification> ;
