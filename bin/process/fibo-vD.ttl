fibo-v-sec:PlannedAmortizationClassBond
        rdf:type                skos:Concept ;
        rdfs:isDefinedBy        fibo-sec-dbt-ab-cmo:PlannedAmortizationClassBond ;
        dct:isPartOf            <https://spec.edmcouncil.org/fibo/SEC/AboutSEC/SECSpecification> ;
        skos:broader            fibo-v-sec:AgencyCMO ;
        skos:definition         "Planned Amortization Class tranche.This is a tranche where the principal payment must follow a certain schedule." ;
        skos:prefLabel          "Planned Amortization Class Bond"@en ;
        fibo-v-sec:maximumPrepaymentSpeed  fibo-v-sec:PaymentSpeed ;
